{
  "version": "2017-02-28",
  "operation": "PutItem",
  "key": {
    "id": $util.dynamodb.toDynamoDBJson($ctx.request.headers.x-api-key),
    "whenclick": $util.dynamodb.toDynamoDBJson($ctx.args.input.whenclick),
  },
  "attributeValues": $util.dynamodb.toMapValuesJson($ctx.args.input),
  "condition": {
    "expression": "attribute_not_exists(#id) AND attribute_not_exists(#whenclick)",
    "expressionNames": {
      "#id": "id",
      "#whenclick": "whenclick",
    },
  },
}